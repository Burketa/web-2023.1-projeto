{{> header}}

<main class="container my-5 pt-5">
    <div class="row">
        <div class="col-12">
            <h1>{{home.title}}</h1>
            <h2>{{home.subtitle}}</h2>
            <p>{{home.description}}</p>
        </div>
    </div>

    <div class="row justify-content-center">
        <div class="col-12">
            <a class="btn btn-primary mx-2" href={{details.url}}>{{details.link}}</a>
            <a class="btn btn-primary mx-2" href={{tools.url}}>{{tools.link}}</a>
            <a class="btn btn-primary mx-2" href={{developer.url}}>{{developer.link}}</a>
            <a class="btn btn-primary mx-2" href={{contact.url}}>{{contact.link}}</a>
        </div>
    </div>
</main>

{{> footer}}